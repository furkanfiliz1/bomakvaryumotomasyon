{
  "name": "figopara",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "start-test": "vite --mode test --port 3000 --open --host",
    "build-test": "vite build --mode test",
    "start-qa": "vite --mode qa --port 3000 --open --host",
    "build-qa": "vite build --mode qa",
    "start-demo": "vite --mode demo --port 3000 --open",
    "build-demo": "vite build --mode demo",
    "start-production": "vite --mode production --port 3000 --open",
    "build-production": "vite build --mode production",
    "build-release": "npm run build-production && npm run build-demo",
    "dev": "vite --port 3000",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "ts-check": "npx tsc",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "preview": "vite preview",
    "generate-icons": "node setup.cjs",
    "test": "vitest run",
    "test-watch": "vitest --watch",
    "test-coverage": "vitest --coverage",
    "prepare": "husky install"
  },
